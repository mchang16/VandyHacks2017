{
    "name":"Ads to Memes",
    "version":"1.0",
    "manifest_version": 2,
    "description":"Chrome extension that blocks ads and replaces them with memes",
    "permissions": ["http://*/"],
    "browser_action":{
        //"default_icon":"icon.png"
        "default_popup":"popup.html"

    },
    "background": {
    "scripts": ["background.js"]
        
    },
    
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["/jquery-3.2.1.js", "content.js"],
        "run_at" : "document_start"
      }
    ],
    "web_accessible_resources": [
    "cats.jpg"
    "memes/%20(1).jpg",
    "memes/%20(2).jpg",
    "memes/%20(3).jpg",
    "memes/%20(4).jpg",
    "memes/%20(5).jpg",
    "memes/%20(6).jpg",
    "memes/%20(7).jpg",
    "memes/%20(8).jpg",
    "memes/%20(9).jpg",
    "memes/%20(10).jpg",
    "memes/%20(11).jpg",
    "memes/%20(12).jpg",
    "memes/%20(13).jpg",
