{
    "name":"Ads to Memes",
    "version":"1.0",
    "manifest_version":2,
    "description":"Chrome extension that blocks ads and replaces them with memes",
    "permissions": ["http://*/"],
    "browser_action":{
        //"default_icon":"icon.png"
        "default_popup":"popup.html"

    },
    "background": {
    "scripts": ["background.js"]
    },
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["content.js"],
        "run_at" : "document_start"
      }
    ]
}
